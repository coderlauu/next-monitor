{
    "import": [""],
    "caseSensitive": false, // 忽略大小写
    "dictionaries": ["custom-words"],
    "dictionaryDefinitions": [
        {
            "name": "custom-words",
            "path": "./.cspell/custom-words.txt",
            "addWords": true
        }
    ],
    "ignorePaths": [
        "**/node_modules/**",
        "**/dist/**",
        "**/lib/**",
        "**/docs/**",
        "**/stats.html",
        "**/language/**",
        "**/language.ts",
        "**/package.json",
        "**/*.md",
        "eslint.config.js",
        "packages/browser-utils/src/metrics/**/*",
        ".gitignore"
    ]
}
